{
  "name": "@route-map/react",
  "version": "0.0.5",
  "author": {
    "email": "ali.gheisar.dev@gmail.com",
    "name": "Ali Gheisar",
    "url": "https://github.com/aligheisar"
  },
  "description": "A simple navigation configuration system for defining routes and rendering UI across different contexts in your react app",
  "repository": {
    "type": "git",
    "url": "https://github.com/aligheisar/route-map",
    "directory": "packages/react"
  },
  "homepage": "https://github.com/aligheisar/route-map/tree/main/packages/react#readme",
  "keywords": [
    "route",
    "react",
    "navigation"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint ."
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "react": "^19.2.0",
    "tsup": "^8.5.1",
    "typescript": "5.9.2"
  }
}
